																	<xs:documentation>Сумма на финансовый год</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="firstYearSum" type="base:moneyPosNegMaxLengthToPoint18Type">
																<xs:annotation>
																	<xs:documentation>Сумма на первый год</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="secondYearSum" type="base:moneyPosNegMaxLengthToPoint18Type">
																<xs:annotation>
																	<xs:documentation>Сумма на второй год</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="subsecYearsSum" type="base:moneyPosNegMaxLengthToPoint18Type" minOccurs="0">
