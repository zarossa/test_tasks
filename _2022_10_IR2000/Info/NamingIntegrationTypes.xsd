									<xs:documentation>Внеплановая проверка (рассмотрение жалобы)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="unplannedCheck" type="zfcs_checkResultUnplannedCheckType">
								<xs:annotation>
									<xs:documentation>Внеплановая проверка</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="plannedCheck" type="zfcs_checkResultPlannedCheckType">
								<xs:annotation>
									<xs:documentation>Плановая проверка</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:choice>
					</xs:complexType>
				</xs:element>
				<xs:element name="complaint" type="zfcs_checkResultComplaintType">
