			<xs:documentation>Сообщение об отказе в принятии на контроль</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="zfcs_control99ConfirmType">
				<xs:sequence>
					<xs:element name="createDate" type="xs:dateTime">
