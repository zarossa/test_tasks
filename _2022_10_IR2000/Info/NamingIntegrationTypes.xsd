													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="valueSet">
						<xs:annotation>
							<xs:documentation>Набор конкретных значений характеристики</xs:documentation>
