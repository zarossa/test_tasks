															<xs:minLength value="1"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="documentDate" type="xs:date">
													<xs:annotation>
														<xs:documentation>Дата документа</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="changeInfo" type="zfcs_longTextMinType">
										<xs:annotation>
											<xs:documentation>Информация об изменении</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="suppliers" minOccurs="0">
