					<xs:documentation>Номер проверки в составе плана проверок, сформированный контрольным органом</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="regNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер проверки в составе плана проверок (согласно ПП РФ №1148)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="25"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="checkSubject" type="zfcs_checkSubjectPlanType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Субъект контроля</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orders" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Объект контроля "Заказ/Закупка"</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="zfcs_unplannedCheckObjectType">
							<xs:sequence minOccurs="0">
								<xs:element name="info" type="zfcs_longTextType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Наименование заказа/закупки</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="purchasePlacingDate" type="xs:dateTime" minOccurs="0">
