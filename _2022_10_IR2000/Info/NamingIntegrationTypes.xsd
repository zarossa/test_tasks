				</xs:annotation>
			</xs:element>
			<xs:element name="repRegNumber" type="zfcs_repRegNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Реестровый номер записи РЕП</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="publishDate" type="xs:dateTime" minOccurs="0">
