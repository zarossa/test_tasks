				</xs:annotation>
			</xs:element>
			<xs:sequence minOccurs="0">
				<xs:element name="act" type="zfcs_checkResultActType">
					<xs:annotation>
						<xs:documentation>Акт</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="actPrescription" type="zfcs_checkResultPrescriptionType" minOccurs="0">
