							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="customerInfo" type="zfcs_control99CustomerInfoType">
				<xs:annotation>
					<xs:documentation>Сведения о заказчике</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="placerInfo" minOccurs="0">
