					<xs:documentation>Номер контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="signDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>Дата заключения контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="foundation">
