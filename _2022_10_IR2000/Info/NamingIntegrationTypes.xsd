														<xs:sequence>
															<xs:element name="documentInfoList" type="zfcs_penalty.documentInfoList">
																<xs:annotation>
																	<xs:documentation>Сведения о документах, подтверждающих факт отмены (возврата) неустойки</xs:documentation>
																</xs:annotation>
