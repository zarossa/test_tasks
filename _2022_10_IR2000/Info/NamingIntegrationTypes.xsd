																	<xs:element name="name" type="zfcs_longTextMinType" minOccurs="0">
																		<xs:annotation>
																			<xs:documentation>Наименование </xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="startCost" type="base:moneyPositiveType" minOccurs="0">
																		<xs:annotation>
																			<xs:documentation>Предложение стоимости работ</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																</xs:sequence>
