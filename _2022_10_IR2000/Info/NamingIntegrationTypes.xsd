							<xs:documentation>Дата/время формирования сообщения</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="violations">
						<xs:annotation>
							<xs:documentation>Выявленные в процессе обработки ошибки и предупреждения</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="violation" type="zfcs_violationType" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Ошибка (предупреждение)</xs:documentation>
									</xs:annotation>
