											<xs:documentation>Итоговый показатель по коду ОКС / ОНИ</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="documentLink" type="zfcs_control99DocLinkType" minOccurs="0">
