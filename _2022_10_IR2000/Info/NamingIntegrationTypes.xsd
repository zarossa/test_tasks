														</xs:annotation>
													</xs:element>
													<xs:element name="vat" type="xs:boolean">
														<xs:annotation>
															<xs:documentation>Включая НДС / без НДС </xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="contract">
