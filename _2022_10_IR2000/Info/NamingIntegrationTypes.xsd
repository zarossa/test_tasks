	<!--Контрольные реестры (РЖ, РВП, РПП, РРК) - интеграция для ОВК. Основные документы-->
	<xs:complexType name="zfcs_eventPlanType">
		<xs:annotation>
			<xs:documentation>План контрольных мероприятий</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="commonInfo" type="zfcs_checkPlanCommonInfoType">
				<xs:annotation>
					<xs:documentation>Общая информация плана</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="startStage" type="zfcs_stageType">
				<xs:annotation>
					<xs:documentation>Начало периода планирования</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="endStage" type="zfcs_stageType">
				<xs:annotation>
					<xs:documentation>Окончание периода планирования</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="checkList">
				<xs:annotation>
					<xs:documentation>Список проверок</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="checkInfo" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Информация по проверке</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="zfcs_checkPlanCheckInfoType">
										<xs:sequence>
											<xs:element name="eventType" type="zfcs_eventType">
												<xs:annotation>
													<xs:documentation>Вид планового контрольного мероприятия
Доступные значения:
PP-Плановая проверка;
PR-Плановая ревизия;
PO–Плановое обследование</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="inspectionType" type="zfcs_inspectionTypeType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Вид плановой проверки
Доступные значения:
V-Выездная проверка;
K-Камеральная проверка;
VV–Встречная проверка в рамках выездной проверки;
VK–Встречная проверка в рамках камеральной проверки</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="surveyType" type="zfcs_surveyType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Вид планового обследования
Доступные значения:
OV-Обследование в рамках выездной проверки;
OK-Обследование в рамках камеральной проверки;
OR-обследование в рамках ревизии</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="parentRegNumber" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Номер связанной проверки в составе плана проверок (согласно ПП РФ №1148)</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:minLength value="1"/>
														<xs:maxLength value="25"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="parentCheckNumber" type="zfcs_checkNumberType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Номер связанной проверки в составе плана проверок, сформированный контрольным органом</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="printForm" type="zfcs_printFormType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Печатная форма плана</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="extPrintForm" type="zfcs_extPrintFormType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Электронный документ, полученный из внешней системы</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemeVersion" type="base:schemeVersionType" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="zfcs_eventPlanProjectType">
		<xs:annotation>
			<xs:documentation>Проект плана контрольных мероприятий</xs:documentation>
