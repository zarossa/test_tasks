															<xs:documentation>Дата документа</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="damagePayments" minOccurs="0" maxOccurs="unbounded">
