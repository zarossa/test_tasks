					<xs:documentation>Номер государственного контракта по государственному оборонному заказу</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="controlAuthorityInfo" type="zfcs_control99ControlAuthorityInfoType">
				<xs:annotation>
					<xs:documentation>Сведения об органе контроля</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commonInfo">
				<xs:annotation>
