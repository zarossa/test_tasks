			<xs:element name="number" type="zfcs_contract.Number2015Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="defenseContractNumber" type="zfcs_contract.defenseNum2015Type" minOccurs="0">
