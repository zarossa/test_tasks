			<xs:element name="attachments" type="zfcs_attachmentListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Прикрепленные файлы</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="modificationInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Описание изменения</xs:documentation>
