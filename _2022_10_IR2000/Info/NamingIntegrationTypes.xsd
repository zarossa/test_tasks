			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="zfcs_contractProcedureType">
		<xs:annotation>
			<xs:documentation>Информация об исполнении (расторжении) контракта</xs:documentation>
		</xs:annotation>
