	<!--Результаты аудита (РРА)-->
	<xs:simpleType name="zfcs_auditResultTypeEnum">
		<xs:annotation>
			<xs:documentation>Вид обобщенной информации:
E - Результаты экспертно-аналитической деятельности;
C - Результаты контрольной деятельности;
I - Результаты информационной деятельности;
O - Результаты иной деятельности;
R - Результаты аудита  привлечения подрядных организаций к выполнению работ по капитальному ремонту многоквартирных домов.
</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="E"/>
			<xs:enumeration value="C"/>
			<xs:enumeration value="I"/>
			<xs:enumeration value="O"/>
			<xs:enumeration value="R"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="zfcs_auditResultType">
		<xs:annotation>
			<xs:documentation>Обобщенная информация о результатах деятельности органа аудита в сфере закупок</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="externalId" type="base:externalIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Внешний идентификатор документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="versionNumber" type="zfcs_revisionNumType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер редакции</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="registryNum" type="base:purchaseNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Реестровый номер</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="docPublishDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата публикации документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="firstPublishDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата публикации документа первой редакции</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="publishOrg" type="zfcs_organizationInfoType">
				<xs:annotation>
					<xs:documentation>Орган аудита в сфере закупок, размещающий обобщенную информацию</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="href" type="base:hrefType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Гиперссылка на опубликованные сведения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="printForm" type="zfcs_contract.printFormType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Печатная форма</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="extPrintForm" type="zfcs_extPrintFormType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Электронный документ, полученный из внешней системы</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="zfcs_auditResultTypeEnum">
				<xs:annotation>
					<xs:documentation>Вид обобщенной информации:
E - Результаты экспертно-аналитической деятельности;
C - Результаты контрольной деятельности;
I - Результаты информационной деятельности;
O - Результаты иной деятельности;
R - Результаты аудита  привлечения подрядных организаций к выполнению работ по капитальному ремонту многоквартирных домов.
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" type="zfcs_longTextMinType">
				<xs:annotation>
					<xs:documentation>Наименование обобщенной информации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="document" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Реквизиты документа</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="type" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Вид документа</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="100"/>
									<xs:minLength value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="date" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Дата</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="number" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Номер</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="100"/>
									<xs:minLength value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="period" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Исследуемый период</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="start" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Дата начала</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="end" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Дата окончания</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="addInfo" type="zfcs_longTextMinType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дополнительная информация</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="action" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Информация о мероприятии</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="auditOrg" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Орган аудита в сфере закупок, размещающий обобщенную информацию</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:choice>
									<xs:element name="registered" type="zfcs_organizationInfoWithOgrnType">
										<xs:annotation>
											<xs:documentation>Зарегистрированная организация</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="nonregistered">
										<xs:annotation>
											<xs:documentation>Незарегистрированная организация</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="fullName" type="oos:zfcs_longTextType">
													<xs:annotation>
														<xs:documentation>Полное наименование</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="OGRN" type="base:ogrnCodeType">
													<xs:annotation>
														<xs:documentation>ОГРН организации</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="OKTMO" type="zfcs_OKTMORef">
													<xs:annotation>
														<xs:documentation>ОКТМО организации</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="subjects">
							<xs:annotation>
								<xs:documentation>Предметы мероприятия</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="subject" type="zfcs_auditActionSubjectsRef" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>Предмет мероприятия</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="objects">
							<xs:annotation>
								<xs:documentation>Объекты мероприятия</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="object" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>Объект мероприятия</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:choice>
													<xs:element name="registered" type="zfcs_organizationInfoWithOgrnType">
														<xs:annotation>
															<xs:documentation>Зарегистрированная организация</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="nonregistered">
														<xs:annotation>
															<xs:documentation>Незарегистрированная организация</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="fullName" type="zfcs_longTextType">
																	<xs:annotation>
																		<xs:documentation>Полное наименование</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="OGRN" type="base:ogrnCodeType">
																	<xs:annotation>
																		<xs:documentation>ОГРН организации. Элемент не используется в импорте</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="oktmo" type="zfcs_OKTMORef"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:choice>
												<xs:element name="violations" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Выявленные нарушения, недостатки</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="violation" type="zfcs_longTextMinType" maxOccurs="unbounded">
																<xs:annotation>
																	<xs:documentation>Нарушение</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="period" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Срок проведения мероприятия</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="start" type="xs:dateTime" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Дата начала</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="end" type="xs:dateTime" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Дата окончания</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="attachments" type="zfcs_attachmentListType" minOccurs="0">
