					<xs:element name="attachments" type="zfcs_attachmentListType">
						<xs:annotation>
							<xs:documentation>Прикрепленные документы</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="zfcs_notificationEFType">
