			<xs:documentation>Основание для решения по результатам общественного обсуждения</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="code">
				<xs:annotation>
					<xs:documentation>Код </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="name">
				<xs:annotation>
					<xs:documentation>Наименование</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="150"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_decisionRef">
		<xs:annotation>
			<xs:documentation>Решение по общественному обсуждению</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="code">
				<xs:annotation>
					<xs:documentation>Код </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="name">
				<xs:annotation>
					<xs:documentation>Наименование</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="150"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_timelineViolationType">
		<xs:annotation>
			<xs:documentation>Уведомление о нарушении контроля времени получения подтверждения</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="refId">
				<xs:annotation>
					<xs:documentation>Идентификатор обработанного пакета данных</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="36"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemeVersion" type="base:schemeVersionType" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!--Общественные обсуждения крупных закупок (ООКЗ). Основные документы-->
	<xs:complexType name="zfcs_publicDiscussionLargePurchasePhase1Type">
		<xs:annotation>
			<xs:documentation>Информация о первом этапе обязательного общественного обсуждения крупной закупки (ООКЗ)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="commonInfo">
				<xs:annotation>
					<xs:documentation>Общая информация обсуждения</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="zfcs_publicDiscussionCommonInfoType">
							<xs:sequence>
								<xs:element name="publicDiscussionPhase1Num" type="base:publicDiscussionNumType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Реестровый номер первого этапа общественного обсуждения</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="phase1" type="zfcs_publicDiscussionPhase1Type">
				<xs:annotation>
					<xs:documentation>Первый этап обсуждения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="attachments" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Документы</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="attachment" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Документ</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="zfcs_attachmentType">
										<xs:sequence>
											<xs:element name="placingDate" type="xs:dateTime" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Дата размещения документа. Элемент не используется при приеме данных.</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="printForm" type="zfcs_printFormType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Печатная форма документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="extPrintForm" type="zfcs_extPrintFormType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Электронный документ, полученный из внешней системы</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemeVersion" type="base:schemeVersionType" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="zfcs_publicDiscussionLargePurchasePhase2Type">
		<xs:annotation>
