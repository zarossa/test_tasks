				</xs:annotation>
			</xs:element>
			<xs:element name="guaranteeInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о независимой гарантии (для печатной формы)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="bank" type="zfcs_bankGuaranteeOrganizationType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Сведения о гаранте, выдавшем гарантию</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="placingOrg" type="zfcs_bankGuaranteeOrganizationType" minOccurs="0">
