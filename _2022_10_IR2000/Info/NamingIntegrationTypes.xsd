						</xs:element>
						<xs:element name="shortStreet" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Улица, введенная вручную</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="200"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="street" type="zfcs_kladrType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Улица в классификаторе КЛАДР</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="zip" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Почтовый индекс</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="6"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="postalAddress" type="zfcs_longTextType" minOccurs="0">
