											<xs:documentation>Информация об исполнении контракта</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="endDate" type="xs:date">
													<xs:annotation>
														<xs:documentation>Дата окончания исполнения этапа</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="ordinalNumber" type="xs:int">
													<xs:annotation>
														<xs:documentation>Номер исполнения в рамках этапа</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="contractTermination" type="xs:boolean" fixed="true">
										<xs:annotation>
											<xs:documentation>Информация о расторжении контракта</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="contractBankGuaranteeProcedure" type="xs:boolean" fixed="true">
										<xs:annotation>
											<xs:documentation>Информация об исполнении обязательств по предоставленной гарантии </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="refundOverpaymentsInfo" type="xs:boolean" fixed="true">
										<xs:annotation>
											<xs:documentation>Информация о возврате поставщиком (подрядчиком, исполнителем) переплаты по контракту</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="contractInvalidation" type="xs:boolean" fixed="true">
										<xs:annotation>
											<xs:documentation>Информация о признании контракта недействительным</xs:documentation>
