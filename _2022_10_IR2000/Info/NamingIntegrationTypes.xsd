					<xs:documentation>Основание заключения контракта</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="order">
							<xs:annotation>
								<xs:documentation>Заказ размещался в ЕИС</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="purchaseNumber" type="base:purchaseNumberType">
										<xs:annotation>
											<xs:documentation>Номер закупки</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="foundationProtocolNumber" type="zfcs_documentNumberType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Номер протокола-основания</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<!--xs:element name="реквизиты письма КО с согласованием размещения у единственного поставщика, бывают случаи просто указания на пункты 94-ФЗ"/-->
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="customer" type="zfcs_organizationRef">
				<xs:annotation>
					<xs:documentation>Заказчик</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="price" type="base:moneyPositiveType">
				<xs:annotation>
					<xs:documentation>Цена контракта
(в валюте контракта)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="priceRUR" type="base:moneyPositiveType" minOccurs="0">
