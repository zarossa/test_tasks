								<xs:restriction base="xs:string">
									<xs:maxLength value="200"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="region" type="zfcs_kladrType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Регион в классификаторе КЛАДР</xs:documentation>
							</xs:annotation>
